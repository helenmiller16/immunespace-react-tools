{
  "name": "immunespace-react-tools",
  "version": "1.0.0",
  "description": "ImmuneSpace React tools",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run clean && better-npm-run build:prod",
    "build-prod": "npm run build",
    "clean": "better-npm-run clean",
    "test": "cross-env NODE_ENV=test jest",
    "storybook": "start-storybook",
    "build:prod": "webpack -p",
    "start": "node server.js",
    "dev": "webpack-dev-server"
  },
  "betterScripts": {
    "build:watch": {
      "command": "webpack-dev-server --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack/prod.config.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf resources/web/DataFinder/gen && rimraf resources/views/*.*"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helenmiller16/immunespace-react-tools.git"
  },
  "author": "Helen Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helenmiller16/immunespace-react-tools/issues"
  },
  "dependencies": {
    "@babel/core": "7.4.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "babel-loader": "8.0.6",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/helenmiller16/immunespace-react-tools#readme",
  "devDependencies": {
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "better-npm-run": "0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "sass-loader": "^9.0.2",
    "ts-loader": "6.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  }
}
